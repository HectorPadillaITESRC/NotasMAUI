<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="NotasMAUI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:NotasMAUI"
    xmlns:views="clr-namespace:NotasMAUI.Views"
    Shell.FlyoutBehavior="Disabled"
    Title="NotasMAUI"
    Shell.TitleColor="White"
    Shell.BackgroundColor="#4b461e"
    Shell.TabBarBackgroundColor="#4b461e"
    xmlns:viewmodels="clr-namespace:NotasMAUI.ViewModels"
    >
    <Shell.BindingContext>
        <viewmodels:UsuariosViewModel></viewmodels:UsuariosViewModel>
    </Shell.BindingContext>


    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate views:LoginView}"
        Route="Login" />

  






</Shell>
